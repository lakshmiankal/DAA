def quick_sort(arr, low, high):
    if low < high:
        pi = partition(arr, low, high)
        print(f"Array after partitioning with pivot {arr[pi]}: {arr}")
        quick_sort(arr, low, pi - 1)
        quick_sort(arr, pi + 1, high)

def partition(arr, low, high):
    pivot = arr[low]
    left = low + 1
    right = high

    done = False
    while not done:
        while left <= right and arr[left] <= pivot:
            left = left + 1
        while arr[right] >= pivot and right >= left:
            right = right - 1
        if right < left:
            done = True
        else:
            arr[left], arr[right] = arr[right], arr[left]

    arr[low], arr[right] = arr[right], arr[low]
    return right

test_cases = [
    ([10, 16, 8, 12, 15, 6, 3, 9, 5], "Test Case 1"),
    ([12, 4, 78, 23, 45, 67, 89, 1], "Test Case 2"),
    ([38, 27, 43, 3, 9, 82, 10], "Test Case 3")
]
for arr, case_name in test_cases:
    print(f"{case_name}:")
    quick_sort(arr, 0, len(arr) - 1)
    print(f"Sorted array: {arr}\n")
